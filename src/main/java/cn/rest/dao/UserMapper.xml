<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="cn.rest.dao.UserDao">
	<resultMap type="cn.rest.entity.User" id="userResult">
		<id property="userId" column="fb_user_id" />
		<result property="userCardId" column="fb_user_card_id" />
		<result property="userName" column="fb_user_name" />
		<result property="userCardPhoto" column="fb_user_card_photo" />
		<result property="userPhone" column="fb_user_phone" />
		<result property="userStatus" column="fb_user_status" />
		<result property="userPassword" column="fb_user_password" />
		<result property="createAt" column="create_at" />
		<result property="updateAt" column="update_at" />
	</resultMap>
	<insert id="insert" parameterType="cn.rest.entity.User"
		useGeneratedKeys="true" keyProperty="userId">
		insert into `fb_user`(
		`fb_user_card_id`,
		`fb_user_name`,
		`fb_user_card_photo`,
		`fb_user_phone`,
		`fb_user_status`,
		`fb_user_password`)
		values(
		#{userCardId,jdbcType=VARCHAR},
		#{userName,jdbcType=VARCHAR},
		#{userCardPhoto,jdbcType=VARCHAR},
		#{userPhone,jdbcType=VARCHAR},
		0,
		#{userPassword,jdbcType=VARCHAR}
		)
	</insert>
</mapper>